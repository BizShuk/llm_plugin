{
    // Gemini CLI: https://geminicli.com/docs/tools/mcp-server/
    "mcpServers": {
        // copy each server to settings.json
        "senera": { // https://github.com/oraios/serena
            "command": "uvx",
            "args": [
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server"
            ],
            "trust": false
        },
        "docker": { // https://github.com/QuantGeekDev/docker-mcp
            "command": "uvx",
            "args": [
                "docker-mcp"
            ]
        },
        "gopls": { // https://go.dev/gopls/features/mcp
            "command": "gopls",
            "args": [
                "mcp"
            ]
        },
        "github": { // https://github.com/github/github-mcp-server
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "GITHUB_PERSONAL_ACCESS_TOKEN",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "<token>"
            }
        },
        "dbhub": { // https://dbhub.ai/config/command-line#dsn
            "type": "stdio",
            "command": "npx",
            "args": [
                "@bytebase/dbhub@latest",
                "--transport",
                "stdio",
                "--config=./config/db.toml"
            ]
        },
        "yt-dlp": {
            "command": "npx",
            "args": [
                "-y",
                "@kevinwatt/yt-dlp-mcp@latest"
            ]
        }
    }
}